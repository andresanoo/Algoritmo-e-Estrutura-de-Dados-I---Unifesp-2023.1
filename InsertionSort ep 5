#include <stdio.h>
#include <time.h>
#define tamanho 20000

void insertionsort (int tam, int vet[]) {
    int i, j, aux;
    for (j = 1; j < tam; j++){
        aux = vet[j];
        for (i = j-1; i >= 0 && vet[i] > aux; i--){
            vet[i+1] = vet[i];
        }
        vet[i+1] = aux;
    }
}

int main(){
    int i, vetor[tamanho];
    clock_t tempo;

    srand(time(NULL));

    for (i = 0; i < tamanho; i++){
        vetor[i] = rand();
    }

    tempo = clock();

    insertionsort(tamanho, vetor);

    tempo = clock() - tempo;

    for (i = 0; i < tamanho; i++){
        printf("%d ", vetor[i]);
    }

    printf("\n%lf\n", ((double)tempo)/((CLOCKS_PER_SEC/1000)));

return 0;
}
